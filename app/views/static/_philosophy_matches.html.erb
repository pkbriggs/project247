<div class="row">
  <!-- First Match -->
  <div class="col-md-4">
    <a class="match_card" href="/match_detail?match=1">
      <div class="match_card_img_container">
        <%= image_tag "market_background.jpg", class: "img-responsive" %>
      </div>

      <div class="match_card_content">
        <h1 class="serif_text top_header no_margin_bottom"><b><%= session[:top_matches_1_name] %></b></h1>
        <ul class="list-inline match_card_content_detail">
          <li class="no_padding_left match_percent olive_text">98% Match</li>
          <li class="address no_padding_right olive_text"><i class="fa fa-map-marker"></i></li>
          <li class="address no_padding_left olive_text">4.2mi</li>
          <%= render partial: "static/hover", :locals => {:match_num => 1} %>
        </ul>
        <p>
          Lorem ipsum sit amet, consectur adipiscing elit, sed do eiusmod tempor inciditunt ut labore et dolore magnum.
        </p>
        <div class="yelp_review_container">
          <%= render partial: "static/stars", :locals => {:num => 3, :half_present => true} %>
        </div>
      </div>
      <div class="match_card_bottom">
        <blockquote>
          <p>My business philosophy is very similar to...</p>
          <footer>Founder, <cite title="Source Title">Businessman #1</cite></footer>
        </blockquote>

        <!-- <a class="block_element button" href="/match_detail?name=Marcelo's%20Pizza">Learn More</a> -->
      </div>
    </a>
  </div>

