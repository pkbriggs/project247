<div class="backdrop full_page_backdrop">
  <div class="row">
    <div class="overlay_container col-md-6 col-md-offset-3">
      <h2 class="serif_text no_margin_top">Great! We've found this match online. Is this you?</h2>

      <div class="possible_name_match">
        <div class="possible_name_match_detail_container">
          <div class="possible_name_match_details">
            <h2 class="possible_name_match_name serif_text">Sweet Inspiration</h2>
            <p class="possible_name_match_address">555 Main Street, San Francisco, CA, 92301</p>
          </div>
        </div>
      </div>

      <div class="yes_no_buttons">
        <%= link_to "no", "/quiz1_name_matches", method: :post, class: "yes_no_button button big" %>
        <%= link_to "yes", "/quiz1_name_matches", method: :post, class: "yes_no_button button big" %>
      </div>

    </div>
  </div>
</div>



